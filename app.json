{
  "expo": {
    "name": "coba-pertama-kali-react-native",
    "slug": "coba-pertama-kali-react-native",
    "version": "1.0.0",
    "scheme": "coba-pertama-kali-react-native",
    "orientation": "portrait",
    "icon": "./assets/freyaku.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/freyaku.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#7F00FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "black"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
